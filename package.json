{
  "name": "magnet-plugin-react",
  "version": "0.0.2",
  "description": "Magnet plugin for react support",
  "engines": {
    "node": ">=7.6.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src",
    "render.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pragmaticivan/magnet-plugin-react.git"
  },
  "scripts": {
    "build": "babel --presets es2015 -d lib/ src/",
    "format": "prettier --single-quote --no-bracket-spacing --trailing-comma es5 --write \"{src/**/*.js}\" && eslint --fix \"{src/**/*.js}\"",
    "lint": "eslint src/**/*.js",
    "test": "jest"
  },
  "author": "Ivan Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pragmaticivan/magnet-plugin-react/issues"
  },
  "homepage": "https://github.com/pragmaticivan/magnet-plugin-react#readme",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.0",
    "metal": "^2.13.2",
    "metal-assertions": "^2.13.2",
    "node-esapi": "0.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "peerDependencies": {
    "magnet": ">= 2.1.0 < 3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "eslint": "^4.7.2",
    "eslint-config-liferay": "^1.0.2",
    "jest": "^21.2.1",
    "magnet": "^2.1.0",
    "prettier": "^1.7.2"
  }
}
